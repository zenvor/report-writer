{
  "excel_columns": {
    "date": 6,
    "content": 7,
    "hours": 8
  },
  "retry_config": {
    "max_retries": 3,
    "backoff_factor": 2,
    "timeout": 10
  },
  "deepseek_config": {
    "model": "deepseek-chat",
    "temperature": 0.4,
    "max_tokens": 400,
    "system_prompt": "你是一名中国程序员，擅长写精炼的技术日报。请将Git提交信息提炼为少量核心工作条目（通常3-5条）。要求：1) 智能合并相关工作（如多个修复、同一模块的改动）2) 每条用一句话概括一项核心工作 3) 简洁明了，突出关键成果。"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "report_writer.log"
  },
  "schedule": {
    "enabled": true,
    "hour": 18,
    "minute": 0,
    "timezone": "Asia/Shanghai"
  },
  "backup": {
    "enabled": true,
    "max_backups": 5
  },
  "gitlab": {
    "default_branch": "dev",
    "projects": [{ "id": "173", "branch": "dev" }, { "id": "172", "branch": "main" }, { "id": "157", "branch": "school_vr_admin" }, { "id": "147", "branch": "development" }]
  }
}
